  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whistlegraph/system/issues"
  },
  "homepage": "https://github.com/whistlegraph/system#readme",
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "http-server": "^14.1.0"
  },
  "devDependencies": {
    "parcel": "^2.5.0",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "prettier": "^2.4.1",
    "typescript": "^4.5.4"
  }
