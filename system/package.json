{
  "name": "system",
  "version": "1.0.0",
  "description": "My virtual computer system for loading and running disks.",
  "source": "./public/index.html",
  "scripts": {
    "dev": "http-server public -S -C ../ssl-dev/localhost.pem -K ../ssl-dev/localhost-key.pem -p 443 --cors -c-1",
